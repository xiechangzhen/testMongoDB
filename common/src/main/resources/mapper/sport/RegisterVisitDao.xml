<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yymt.dao.sport.RegisterVisitDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.yymt.entity.sport.RegisterVisitEntity" id="registerVisitMap">
        <result property="id" column="id"/>
        <result property="visitorName" column="visitor_name"/>
        <result property="visitTime" column="visit_time"/>
        <result property="vistorTelephone" column="vistor_telephone"/>
        <result property="identityCard" column="identity_card"/>
        <result property="visitDept" column="visit_dept"/>
        <result property="visitPersonnel" column="visit_personnel"/>
    </resultMap>

<sql id="Base_Column_List" >
id AS id,
visitor_name AS visitorName,
visit_time AS visitTime,
vistor_telephone AS vistorTelephone,
identity_card AS identityCard,
visit_dept AS visitDept,
visit_personnel AS visitPersonnel,
</sql>
</mapper>